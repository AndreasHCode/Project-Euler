package solution;

public class Main {

	public static void main(String[] args) {

		int a = 0;
		int b = 0;
		int c = 0;

		for (int i = 0; i < 10; i++) {
			b = a;
			a++;

			for (int j = a; j < 10; j++) {
				b++;
				double squareSum = Math.pow(a, 2) + Math.pow(b, 2);
				System.out.println(squareSum);
				System.out.println("Root: " + Math.sqrt(squareSum));
				System.out.println("A: " + a);
				System.out.println("B: " + b);
			}

		}

	}
}
