package solution;

public class Main {
	
	public static void main(String[] args) {
		boolean searching = true;
		int number = 0;
		
		while (searching) {
			
			for (int i = 1; i <= 10; i++) {
				if (number % i != 0) {
					System.out.println("Number: " + number " + not Valid");
				}
			}
			
		}
		
	}

}
