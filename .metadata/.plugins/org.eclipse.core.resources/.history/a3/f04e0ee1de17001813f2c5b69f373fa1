package solution;

public class Main {
	
	private static final int UPPER_LIMIT = 20;
	
	public static void main(String[] args) {
		boolean searching = true;
		int number = 10;
		
		while (searching) {
			
			for (int i = 1; i <= 10; i++) {
				if (number % i != 0) {
					break;
				} else if (i == 10) {
					System.out.println("Number: " + number + " is Valid");
					searching = false;
				}
			}
			
			number += 20;
		}
		
	}

}
